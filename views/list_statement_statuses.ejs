<div class='ui container'>
    <div class="ui hidden divider"></div>
    <h2 class="ui header">Document upload status</h2>
    <div class="ui  divider"></div>
    <%statements.forEach(function(doc){%>
	<% if(doc.extracted_data){%>
        <div class='ui icon success message'>
            <i class="check square icon"></i>
            <div class="content">
                <div class="header">
                        <%=doc.details.original_filename%>
                </div>
                <p>Parsing is completed. <a href="/org/<%=req.org.id%>/statement/<%=doc.id%>">View the uploaded document here</a>.</p>
                <p></p>
            </div>
			
		</div>
    <%} else if(new Date() - new Date(doc.createdAt) < 300000){ %>
        <div class='ui icon message'>
            <i class="notched circle loading icon"></i>
            <div class="content">
                <div class="header">
                        <%=doc.details.original_filename%>
                    </div>
                
                <p>Parsing in progress. Check back in a minute</p>
            </div>
        </div>
		
        <%}else{%>
            <div class='ui icon negative message'>
                <i class="x icon"></i>
                <div class="content">
                    <div class="header">
                            <%=doc.details.original_filename%>
                    </div>
                    <p>Not able to parse any line items</p>
                </div>
                
            </div>
        <%}%>
    <%})%>
	
</div>
<script type="text/javascript">
	$(document).ready(function(){
		$('.tabular.menu .item').tab();
	});
</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.21/moment-timezone.min.js"></script>